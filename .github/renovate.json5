{
  extends: [
    "config:base",
    ":disableDependencyDashboard",
    "group:all",
    "schedule:weekly",
    ":semanticCommitTypeAll(chore)"
  ],
  labels: ["automerge"],
  postUpdateOptions: ["gomodTidy"],
  packageRules: [
    {
      matchPackageNames: [
        "bitbucket.org/creachadair/stringset",
        "dominikh/staticcheck-action",
        "github.com/olekukonko/tablewriter",
        "ruby",
        "webrick",
        "jekyll",
      ],
      enabled: false
    }
  ],
  force: {
    constraints: {
      go: "1.24"
    }
  }
}
